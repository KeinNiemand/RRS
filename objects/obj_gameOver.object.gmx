gameTime = obj_controll.gameTime;
modeID = obj_controll.modeID;
//Pause the game and delete pause background sprite
scr_pause(true);

//Creates the button that ends the Game
pMenu[0] = instance_create(room_width/2,room_height,obj_ingameButton)
with (pMenu[0]) {
    text = "End Game";
    spriteColor = c_white;
    onClick  = scr_EndGame;
    image_yscale = 2;
    y-= sprite_height;
}
/*
MAKES THE SCREEN BLACK
Set box variables to draw a intranparent black box
*/
color=c_black;
alpha=1;
//Set Titel of Game Over Menu
text="Game Over"
//Set Text Size
textScale = 1.5;
//set gameEnd (activates the drawing of stats, recived coins, ...) to false
gameEnd = false;
</string>
